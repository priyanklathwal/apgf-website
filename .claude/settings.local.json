{
  "permissions": {
    "allow": [
      "Bash(wc:*)",
      "Bash(python3:*)",
      "Bash(gh:*)",
      "Bash(/opt/homebrew/bin/brew:*)",
      "Bash(/usr/local/bin/brew:*)",
      "Bash(/opt/homebrew/bin/gh auth status)",
      "Bash(/usr/local/bin/gh auth status)",
      "Bash(export PATH=\"/opt/homebrew/bin:$PATH\")",
      "Bash(git init:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(/opt/homebrew/bin/gh repo create apgf-website --public --source=. --push)",
