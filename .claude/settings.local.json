      "Bash(curl:*)",
      "Bash(dig:*)",
      "Bash(/opt/homebrew/bin/gh api repos/priyanklathwal/apgf-website/pages -X PUT --input - 2)",
      "Bash(1 <<'EOF'\n{\"https_enforced\":true,\"source\":{\"branch\":\"master\",\"path\":\"/\"}}\nEOF)",
      "Bash(/opt/homebrew/bin/gh api repos/priyanklathwal/apgf-website/pages)",
